<!DOCTYPE html>
<html>
<head>
  <title>Calendar</title>
  <%= stylesheet_link_tag    "application", media: "all", "data-turbolinks-track" => true %>
  <%= javascript_include_tag "application", "data-turbolinks-track" => true %>
  <%= csrf_meta_tags %>
</head>
<body>

<% if signed_in? %>
	<%= render 'layouts/menu' %>
<% end %>



<div class="container">
	<% unless notice.nil? && alert.nil? %>
		<div class="alert"><%= notice %>
		<%= alert %></div>
	<% end %>
	<div class="row">
		<%= yield %>
	</div>
</div>
</body>
</html>
